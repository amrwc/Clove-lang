/**
 * Test Reflection API
 */

log("Reflection API");
log();


log('const math = reflect("java.lang.Math");');
const math = reflect("java.lang.Math");

log('math.negateExact(10): ', math.negateExact(10));
log('math.round(77.77): ', math.round(77.77));
log();
log();


const string = reflect("java.lang.String", ["example string"]);
const string2 = "other string";
const string3 = "example string";

if (string.compareTo(string3) == 0) {
  log("It's a match!");
} else {
  log("The strings are different.");
}
log();
log();


const integer1 = reflect("java.lang.Integer", ["10"]);
const integer2 = reflect("java.lang.Integer", ["12"]);
log(integer1 + integer2); // Operations on two ValueReflections
log(integer1 - integer2);
log(integer1 * integer2);
log(integer1 / integer2);
log();

log(3 * integer2); // Operation on ValueInteger and ValueReflection
log();

const long1 = reflect("java.lang.Long", ["10"]);
const long2 = reflect("java.lang.Long", ["12"]);
log(long1 + long2);
log(long1 - long2);
log(long1 * long2);
log(long1 / long2);
log(long1 % long2);
log(10 % long2);
log(long1 % 999999999999999); // Parse long literal
log(999999999999999 % long2);
log();
log();
log();


const float1 = reflect("java.lang.Float", [3.14]);
const float2 = reflect("java.lang.Float", [1.62]);
log(float1 * float2); // Operation on two ValueReflections
log(3.14 * float2); // Operations on ValueRational and ValueReflection
log(float2 * 3.14);
log();

const double1 = reflect("java.lang.Double", ["3.14"]);
const double2 = reflect("java.lang.Double", ["1.62"]);
log(double1 * double2);
log(3.14 * double2);
log(double2 * 3.14);
log();
log();
log();


const bool1 = reflect("java.lang.Boolean", [false]);
const bool2 = reflect("java.lang.Boolean", [true]);
log(bool1 || bool2); // Operations on ValueReflection(s)
log(bool1 && bool2);
log(!bool1);

log(!bool1 && bool2); // Operations on ValueBoolean and ValueReflection
log(!bool2 || bool1);
log(false || bool2);
log();
log();
log();


