// Test arrow functions
write("Arrow functions:", "")


// Dave's first challenge:
myop = (x, y) => {
	return x + y
}
write(myop(2, 3), "")


// Nested arrow functions:
anon = (x, y) => {
	anonNested = (a, b) => {
		return a + b
	}
	
	return x + y + anonNested(3, 4)
}
write(anon(1, 2), "")


// Dave's second challenge:
myop2 = (x, y) => {
	return x(22) + y
}
write(myop2(
	(p) => {
		return p * 4
	},
	3
), "")


// Beyond the challenges:
singleNested = (a, b, c) => {
	return a() + b() + c()
}
write(singleNested(
	() => { return 222 },
	() => { return 777 },
	() => { return 2+2 }
))

withArguments = (a, b) => {
	return a(1) + b
}
write(withArguments(
	(x) => { return x + 2 },
	9
))

doubleNested = (a, b, c) => {
	return a(() => { return 44 + 55}) + b + c
}
write(doubleNested(
	(xyz) => { return xyz() + 11 },
	22,
	33
))
