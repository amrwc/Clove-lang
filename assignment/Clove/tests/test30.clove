log("Before appending with resizing: ", arr)
for (let i = 0; i < 10; i++) {
  if (arr->length() + 1 > arr->cap()) {
    arr->resize(arr->cap() + 1)
  }
  arr->append(i)
}
log("After: ", arr)
log()


